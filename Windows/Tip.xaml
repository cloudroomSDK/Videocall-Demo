<Window x:Class="SDKDemo.Tip"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStartupLocation="CenterScreen" Title="提示" Height="150" Width="300" Closing="Window_Closing">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Name="TipLabel" Grid.Row="0" HorizontalContentAlignment="Center"  VerticalContentAlignment="Center" FontSize="18" FontFamily="Microsoft YaHei" ></Label>
    </Grid>
</Window>
