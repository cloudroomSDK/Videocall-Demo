<Window x:Class="VideoCall.StartQueuing"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="开始排队" Height="230" Width="400" Closed="Window_Closed" ResizeMode="NoResize" WindowStyle="SingleBorderWindow" WindowStartupLocation="CenterScreen" ShowInTaskbar="False">
    <Grid Height="150">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="166*" />
            <ColumnDefinition Width="212*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Center" Name="tb_queName" Foreground="#3791E9"></TextBlock>
        <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Name="tb_waitTime">排队中</TextBlock>
        <TextBlock Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Name="tb_position"></TextBlock>
        <Button Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Width="120" Height="27" Name="btnCancel" Click="btnCancel_Click" Background="LightGray">取消</Button>
    </Grid>
</Window>
