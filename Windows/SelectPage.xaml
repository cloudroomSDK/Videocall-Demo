<Window x:Class="SDKDemo.SelectPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Button Name="btn1" Width="200" Height="100" Style="{StaticResource HighlightBtn}" HorizontalAlignment="Center" VerticalAlignment="Center" Click="btn1_click"></Button>
        <Button Name="btn2" Width="200" Height="100" Style="{StaticResource HighlightBtn}" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="1" Click="btn2_click"></Button>
    </Grid>
</Window>
