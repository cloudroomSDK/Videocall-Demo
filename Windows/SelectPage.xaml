<Window x:Class="SDKDemo.SelectPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="功能选择" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Button Width="200" Height="100" Style="{StaticResource HighlightBtn}" HorizontalAlignment="Center" VerticalAlignment="Center" Content="排队叫号" Click="btn_queue_call_click"></Button>
        <Button Width="200" Height="100" Style="{StaticResource HighlightBtn}" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="1" Content="直接呼叫" Click="btn_direct_call_click"></Button>
    </Grid>
</Window>
